{
  "name": "svelte-instantsearch",
  "version": "0.8.0",
  "license": "MIT",
  "description": "Svelte wrapper for Algolia InstantSearch",
  "repository": "aymeric-giraudet/svelte-algolia-instantsearch",
  "keywords": ["svelte", "algolia", "instantsearch", "search", "instantsearch.js"],
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package -o dist",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "release": "shipjs prepare"
  },
  "dependencies": {
    "algoliasearch-helper": "^3.11.2",
    "instantsearch.css": "^8.0.0",
    "instantsearch.js": "^4.49.4"
  },
  "devDependencies": {
    "@sveltejs/kit": "^1.20.4",
    "@sveltejs/package": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "algoliasearch": "^4.14.3",
    "css-loader": "^6.7.4",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte": "^2.32.2",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.10.1",
    "style-loader": "^3.3.3",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.0",
    "vite": "^4.1.1",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "algoliasearch": ">= 3.1 < 5",
    "svelte": ">= 3"
  }
}
